{
  "headers": [
    {
      "source": "/assets/(.*)\\.(?:avif|webp|png|jpg|jpeg|svg|gif|css|js|woff2?)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/index.html",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    }
  ]
}
